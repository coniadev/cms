{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "$lib": [
                "./src/lib"
            ],
            "$lib/*": [
                "./src/lib/*"
            ],
            "$shell": [
                "./src/shell"
            ],
            "$shell/*": [
                "./src/shell/*"
            ]
        }
    }
}
