{
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": ["prettier-plugin-svelte"],
    "printWidth": 100,
    "semi": true,
    "singleAttributePerLine": true,
    "singleQuote": true,
    "svelteSortOrder": "options-scripts-markup-styles",
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false
}
