{
    "name": "conia/cms",
    "description": "Conia Content Management Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ebene fÃ¼nf GmbH",
            "email": "info@ebenefuenf.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "conia/core": "@dev",
        "conia/error": "dev-main",
        "conia/quma": "dev-main",
        "conia/registry": "dev-main",
        "conia/session": "dev-main",
        "conia/sire": "dev-main",
        "gumlet/php-image-resize": "2.0.*",
        "hidehalo/nanoid-php": "^1.1",
        "laminas/laminas-httphandlerrunner": "^2.4",
        "psr/http-message-implementation": "^1.0",
        "symfony/html-sanitizer": "^6.2"
    },
    "require-dev": {
        "conia/chuck-boiler": "dev-main",
        "conia/development": "^1.0",
        "laminas/laminas-diactoros": "^2",
        "phpunit/phpunit": "^10"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../conia/core"
        },
        {
            "type": "path",
            "url": "../../conia/route"
        }
    ],
    "autoload": {
        "psr-4": {
            "Conia\\Cms\\": "src"
        },
        "files": [
            "src/Util/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Conia\\Cms\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
